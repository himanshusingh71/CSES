
#include<bits/stdc++.h>

using namespace std;

 
long long solve(long long num)
{
	if(num == 1 ) return 0;
	if( num == 2 ) return 6;


	 return ((num*num)*(num*num - 1 ) )/2 - 4*((num-1)*(num-2));


}
int main(){

ios_base::sync_with_stdio(false);  
cin.tie(NULL); // flushes cout  
    
  int n;

  cin >> n;

  for(int i=1;i<=n;i++)
  {
  	cout << solve(i) << endl;
  }


}

